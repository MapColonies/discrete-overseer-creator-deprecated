{
  "openapiConfig": {
    "filePath": "bundledApi.yaml",
    "basePath": "/docs",
    "jsonPath": "/api.json",
    "uiPath": "/api"
  },
  "logger": {
    "level": "info"
  },
  "server": {
    "port": "8080"
  },
  "LayerSourceDir": "/layerSources",
  "tilerKafka": {
    "clientId": "discrete-overseer-creator",
    "brokers": "192.168.0.14:9092",
    "topic": "tile-creator",
    "retry": {
      "maxRetryTime": 10000,
      "initialRetryTime": 100,
      "factor": 0.2,
      "multiplier": 2,
      "retries": 3
    },
    "ssl": {
      "rejectUnauthorized": false,
      "ca": null,
      "key": null,
      "cert": null
    }
  },
  "storageServiceURL": "http://localhost:8081",
  "mapPublishingServiceURL": "http://localhost:8082",
  "catalogPublishingServiceURL": "",
  "tiling": {
    "zoomGroups": "0-10,11,12,13,14,15,16,17,18"
  },
  "httpRetry": {
    "attempts": 5,
    "delay": "exponential",
    "shouldResetTimeout": true
  }
}
